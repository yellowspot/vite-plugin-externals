{
  "name": "@yellowspot/vite-plugin-externals",
  "description": "Use to external resources, like webpack externals",
  "version": "0.4.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
